<div {{ attributes }}>
    <input type="text" data-model="query" placeholder="Search for a product...">

    {% if products|length > 0 %}
        <ul>
            {% for product in products %}
                <li>{{ product.name }}</li>
            {% endfor %}
        </ul>
    {% else %}
        <p>No products found.</p>
    {% endif %}
</div>
